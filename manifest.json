{
    "manifest_version": 2,
    "name": "Sweet Home",
    "version": "1.0",
    "description": "This extension will make your home page more beautiful.",
    "permission":["storage"],
    "browser_action":{
        "default_icon":"icon.png",
        "default_popup":"popup.html"
    },
    "icons":{
        "16":"icon16.png",
        "32":"icon32.png",
        "48":"icon48.png",
        "128":"icon128.png"
    },
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["content.js"]
        }
    ]

}